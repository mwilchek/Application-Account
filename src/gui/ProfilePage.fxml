      <TextField editable="false" layoutX="14.0" layoutY="192.0" prefHeight="25.0" prefWidth="75.0" text="Gender:" />
      <TextField id="firstName" editable="false" layoutX="99.0" layoutY="97.0" prefHeight="25.0" prefWidth="153.0" />
      <TextField id="lastName" editable="false" layoutX="99.0" layoutY="128.0" prefHeight="25.0" prefWidth="153.0" />
